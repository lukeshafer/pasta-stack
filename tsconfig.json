{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "astro/tsconfigs/strictest",
    "compilerOptions": {
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/lib/*"
